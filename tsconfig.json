{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ES2020",
    "module": "CommonJS",
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "typeRoots": ["./node_modules/@types", "./src/types"] ,
    "types": ["node", "express"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  // "include": ["src", "prisma"]
  "include": ["src"]
}